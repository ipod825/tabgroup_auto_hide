{
  "manifest_version": 3,
  "default_locale": "en",
  "name": "__MSG_extName__",
  "short_name": "TAH",
  "description": "__MSG_extDescription__",
  "version": "2.3",
  "permissions": [
    "commands",
    "tabs",
    "tabGroups",
    "storage",
    "notifications"
  ],
  "commands": {
    "TAH_NextTab": {
      "description": "__MSG_commandNextTab__"
    },
    "TAH_PreviousTab": {
      "description": "__MSG_commandPreviousTab__"
    },
    "TAH_MoveTabRight": {
      "description": "__MSG_commandMoveTabRight__"
    },
    "TAH_MoveTabLeft": {
      "description": "__MSG_commandMoveTabLeft__"
    },
    "TAH_MoveTabGroupRight": {
      "description": "__MSG_commandMoveTabGroupRight__"
    },
    "TAH_MoveTabGroupLeft": {
      "description": "__MSG_commandMoveTabGroupLeft__"
    },
    "TAH_SendTabToRightGroup": {
      "description": "__MSG_commandSendTabToRightGroup__"
    },
    "TAH_SendTabToLeftGroup": {
      "description": "__MSG_commandSendTabToLeftGroup__"
    },
    "TAH_ToggleAutoHideForCurrentGroup": {
      "description": "__MSG_commandToggleAutoHide__"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png",
    "256": "icons/icon256.png",
    "512": "icons/icon512.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "web_accessible_resources": [
    {
      "resources": [ "logs.html" ],
      "matches": [ "<all_urls>" ]
    }
  ]
}
