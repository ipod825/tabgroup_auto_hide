{
  "manifest_version": 3,
  "default_locale": "en",
  "name": "TabGroupAutoHide",
  "short_name": "TAH",
  "description": "__MSG_package_info__",
  "version": "1.9",
  "permissions": [
    "commands",
    "tabs",
    "tabGroups",
    "storage"
  ],
  "commands": {
    "TAH_NextTab": {
      "description": "__MSG_TAH_NextTab__"
    },
    "TAH_PreviousTab": {
      "description": "__MSG_TAH_PreviousTab__"
    },
    "TAH_MoveTabRight": {
      "description": "__MSG_TAH_MoveTabRight__"
    },
    "TAH_MoveTabLeft": {
      "description": "__MSG_TAH_MoveTabLeft__"
    },
    "TAH_MoveTabGroupRight": {
      "description": "__MSG_TAH_MoveTabGroupRight__"
    },
    "TAH_MoveTabGroupLeft": {
      "description": "__MSG_TAH_MoveTabGroupLeft__"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png",
    "256": "icons/icon256.png",
    "512": "icons/icon512.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
}
