{
  "manifest_version": 3,
  "default_locale": "en",
  "name": "TabGroupAutoHide",
  "short_name": "TAH",
  "description": "__MSG_package_info__",
  "version": "1.5",
  "permissions": [
    "commands",
    "tabs",
    "tabGroups",
    "storage"
  ],
  "commands": {
    "TAH_NextTab": {
      "suggested_key": {
        "default": "Ctrl+L"
      },
      "description": "__MSG_TAH_NextTab__"
    },
    "TAH_PreviousTab": {
      "suggested_key": {
        "default": "Ctrl+H"
      },
      "description": "__MSG_TAH_PreviousTab__"
    },
    "TAH_MoveTabRight": {
      "suggested_key": {
        "default": "Ctrl+Shift+L"
      },
      "description": "__MSG_TAH_MoveTabRight__"
    },
    "TAH_MoveTabLeft": {
      "suggested_key": {
        "default": "Ctrl+Shift+H"
      },
      "description": "__MSG_TAH_MoveTabLeft__"
    },
    "TAH_MoveTabGroupRight": {
      "description": "__MSG_TAH_MoveTabGroupRight__"
    },
    "TAH_MoveTabGroupLeft": {
      "description": "__MSG_TAH_MoveTabGroupLeft__"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
}
